
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produto {
  id                    Int      @default(autoincrement()) @id @unique
  codigo                Int
  nome                  String
  descricao             String
  categoria             String
  precooriginal         Float
  porcentagemdesconto   Int
  precodesconto         Float
}


model Usuario {
  id            Int      @default(autoincrement()) @id @unique
  nome          String
  email         String   @unique
  senha         String
  created_at    DateTime @default(now())  
}
