
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Produto {
  id                    Int      @default(autoincrement()) @id
  codigo                Int
  nome                  String
  descricao             String
  categoria             String
  precooriginal         Float
  porcentagemdesconto   Int
  precodesconto         Float
}

